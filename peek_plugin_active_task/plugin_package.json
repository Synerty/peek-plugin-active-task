{
  "plugin": {
    "title": "Active Task",
    "packageName": "peek_plugin_active_task",
    "version":"0.1.5",
    "buildNumber": "#PLUGIN_BUILD#",
    "buildDate": "#BUILD_DATE#",
    "creator": "Synerty Pty Ltd",
    "website": "www.synerty.com"
  },
  "requiresServices": [
    "server",
    "client",
    "storage"
  ],
  "client": {
    "showHomeLink": false,
    "showInTitleBar": true,
    "titleBarLeft": false,
    "titleBarText": "Tasks",
    "enableAngularFrontend": true,
    "appDir": "_private/client_fe_app",
    "moduleDir": "client_fe_module",
    "assetDir": "_private/client_fe_assets",
    "appModule": "plugin-active-task.module#PluginActiveTaskClientModule",
    "rootModule": {
      "file": "plugin-active-task-root.module",
      "class": "PluginActiveTaskRootModule"
    },
    "rootService": {
      "file": "plugin-active-task-root.service",
      "class": "PluginActiveTaskRootService"
    },
    "icon": null
  },
  "server": {
    "enableAngularFrontend": true,
    "moduleDir": "client_fe_module",
    "appDir": "_private/server_fe_app",
    "appModule": "plugin-active-task.module#PluginActiveTaskAdminModule"
  },
  "storage": {
    "alembicDir": "_private/alembic"
  }
}