{
  "plugin": {
    "title": "Active Task",
    "packageName": "peek_plugin_active_task",
    "version": "0.0.1",
    "buildNumber": "#PLUGIN_BUILD#",
    "buildDate": "#BUILD_DATE#",
    "creator": "Synerty Pty Ltd",
    "website": "www.synerty.com"
  },
  "requiresServices": [
    "server",
    "client",
    "storage"
  ],
  "client": {
    "showPluginHomeLink": false,
    "showPluginInTitleBar": true,
    "titleBarLeft": false,
    "titleBarText": "Tasks",
    "enableAngularFrontend": true,
    "angularFrontendDir": "_private/client_fe",
    "angularMainModule": "plugin-active-task.module",
    "angularRootModule": {
      "file": "plugin-active-task-root.module",
      "class": "PluginActiveTaskClientRootModule"
    },
    "angularRootService": {
      "file": "plugin-active-task-root.service",
      "class": "PluginActiveTaskClientRootService"
    },
    "angularPluginIcon": null
  },
  "server": {
    "enableAngularFrontend": true,
    "angularFrontendDir": "_private/server_fe",
    "angularMainModule": "plugin-active-task.module"
  },
  "storage": {
    "alembicDir": "_private/alembic"
  }
}